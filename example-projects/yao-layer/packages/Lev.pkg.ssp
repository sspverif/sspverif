package Lev {
    params {
        n: Integer,
    }

    state {
    }

    import oracles {
        GETBIT(h: Integer) -> Bool,
        SETBIT(h: Integer, zz: Bool),
    }


    oracle EVAL(j: Integer, l: Integer, r: Integer, op: Table((Bool, Bool), Bool)) {
        zl <- invoke GETBIT(l);
        zr <- invoke GETBIT(r);
        z <- Unwrap(op[(zl, zr)]);
        _ <- invoke SETBIT(j, z);
        return;
    }
}
